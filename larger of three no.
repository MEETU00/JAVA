public class three_number {
    public static void main(String args[]) {
      int a,b,c;
      a=10;
      b=5;
      c=9;
      if(a>b && a>c)
      {
          System.out.println("Largest no is " + a);
      }
      else if(b>a && b>c)
      {
          System.out.println("largest no is " + b);
      }
      else 
          System.out.println("largest no is " + c);
       
    }
}
